    "version": "0.2.0",
    "configurations": [
        {
            "name": "Validate",
            "type": "python",
            "request": "launch",
            "program": "val.py",
            "console": "integratedTerminal",
            "justMyCode": true,
            "args":[
                "--yolo_model", "./runs/yolov5s/weights/best.pt",
            ]
        },
        {
            "name": "Prepare Data",
            "type": "python",
            "request": "launch",
            "program": "prepare_data.py",
            "console": "integratedTerminal",
            "justMyCode": true,
            "args":[
                "--videos_dir", "../datasets/SmartPlane/Videos",
                "--data_dir", "../datasets/SmartPlane",
            ]
        },
        {
            "name": "Train",
            "type": "python",
            "request": "launch",
            "program": "train.py",
            "console": "integratedTerminal",
            "justMyCode": true,
            "args":[
                "--epochs", "1",
            ]
        },
        {
            "name": "Detect",
            "type": "python",
            "request": "launch",
            "program": "detect.py",
            "console": "integratedTerminal",
            "justMyCode": true,
            "args":[
                "--yolo_model", "./runs/yolov5s/weights/best.pt",
                "--source", "../datasets/SmartPlane/images"
            ]
        },
    ]

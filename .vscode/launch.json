        {
            "name": "Validate",
            "type": "python",
            "request": "launch",
            "program": "val.py",
            "console": "integratedTerminal",
            "justMyCode": true,
            "args": [
                "--yolo_model",
                "./runs/train/2022-02-17/yolov8n/weights/best.pt",
                "--name",
                "debug"
            ]
        },
        {
            "name": "Prepare Data",
            "type": "python",
            "request": "launch",
            "program": "prepare_data.py",
            "console": "integratedTerminal",

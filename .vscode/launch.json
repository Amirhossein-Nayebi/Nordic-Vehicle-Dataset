    "version": "0.2.0",
    "configurations": [
        {
            "name": "Data Statistics",
            "type": "python",
            "request": "launch",
            "program": "data_stats.py",
            "console": "integratedTerminal",
            "justMyCode": true,
            "args": [
                "./height(175, 200)-smart_plane.yaml"
            ]
        },
        {
            "name": "Extract Frames",
            "type": "python",
            "request": "launch",
            "program": "./Util/extract_frames.py",
            "console": "integratedTerminal",
            "justMyCode": true,
            "args": [
                "-f",
                "../datasets/SmartPlane/Videos/2022-12-04 Bjenberg 02.MP4",
                "-s",
                "00:03",
                "-e",
                "00:04",
                "-o",
                "test",
            ]
        },
        {
            "name": "Estimate Flight Height",
            "type": "python",
            "request": "launch",
            "program": "estimate_flight_height.py",
            "console": "integratedTerminal",
            "justMyCode": true,
            "args": [
                "../datasets/SmartPlane/Videos/test"
            ]
        },
        {
            "name": "Validate",
            "type": "python",
            "request": "launch",
            "program": "val.py",
            "console": "integratedTerminal",
            "justMyCode": true,
            "args": [
                "--yolo_model",
                "./runs/train/2022-02-17/yolov8n/weights/best.pt",
                "--name",
                "debug"
            ]
        },
        {
            "name": "Prepare Data",
            "type": "python",
            "request": "launch",
            "program": "prepare_data.py",
            "console": "integratedTerminal",
            "justMyCode": true,
            "args": [
                "--source", "../datasets/SmartPlane/Videos/test/",
                "--data_dir", "../datasets/SmartPlane/test",
                "--height_based",
            ]
        },
        {
            "name": "Train",
            "type": "python",
            "request": "launch",
            "program": "train.py",
            "console": "integratedTerminal",
            "justMyCode": false,
            "args": [
                "--epochs",
                "1",
                "--yolo_model",
                "yolov5n",
                "--name",
                "debug",
                "--aug",
            ]
        },
        {
            "name": "Detect",
            "type": "python",
            "request": "launch",
            "program": "detect.py",
            "console": "integratedTerminal",
            "justMyCode": true,
            "args": [
                "--yolo_model",
                "./runs/train/2022-02-17/yolov8n/weights/best.pt",
                "--source",
                "../datasets/SmartPlane/Videos/2022-12-02 Asjo 01_stabilized.MP4",
                "--name",
                "debug",
            ]
        },
        {
            "name": "View Annotations",
            "type": "python",
            "request": "launch",
            "program": "view_annotations.py",
            "console": "integratedTerminal",
            "justMyCode": true,
            "args": [
                "../datasets/SmartPlane/Videos/2022-12-23 Bjenberg 02_stabilized.xml",
            ]
        },
        {
            "name": "View Data",
            "type": "python",
            "request": "launch",
            "program": "view_data.py",
            "console": "integratedTerminal",
            "justMyCode": true,
            "args": [
                "--video_file",
                "../datasets/SmartPlane/Videos/2022-12-03 Nyland 01_stabilized.mp4",
                "--type",
                "test"
            ]
        },
    ]
}